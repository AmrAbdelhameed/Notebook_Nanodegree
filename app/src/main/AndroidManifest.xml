<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.amr.notebook_nanodegree">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/noteicon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="Notebook">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".RegistrationActivity"
            android:label="Registration Now" />
        <activity
            android:name=".ShowContacts"
            android:label="Your Contacts" />
        <activity
            android:name=".AddFriend"
            android:label="Add Contact">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.amr.notebook_nanodegree.ShowContacts" />
        </activity>
        <activity
            android:name=".ForgetPassword"
            android:label="Reset Password" />
    </application>
</manifest>